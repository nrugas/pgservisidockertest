        <Setter Property="FontSize" Value="{telerik1:MaterialResource ResourceKey=FontSize}"/>
        <Setter Property="MinHeight" Value="{StaticResource CalculatorButtonMinHeight}"/>
        <Setter Property="MinWidth" Value="{StaticResource CalculatorButtonMinWidth}"/>
        <Setter Property="Margin" Value="0"/>
        <Setter Property="Padding" Value="0 5"/>
