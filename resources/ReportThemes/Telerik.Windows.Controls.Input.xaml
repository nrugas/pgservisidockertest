                        <telerik:RadButton x:Name="input2_Button" Content="2" Grid.Row="4" Grid.Column="1" Command="{x:Static controls:RadCalculatorCommands.UpdateInput}" CommandParameter="2"/>
                        <telerik:RadButton x:Name="input3_Button" Content="3" Grid.Row="4" Grid.Column="2" Command="{x:Static controls:RadCalculatorCommands.UpdateInput}" CommandParameter="3"/>
                        <telerik:RadButton x:Name="Substract_Button"
