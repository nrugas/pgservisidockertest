                        <telerik:RadButton x:Name="input5_Button" Content="5" Grid.Row="3" Grid.Column="1" Command="{x:Static controls:RadCalculatorCommands.UpdateInput}" CommandParameter="5"/>
                        <telerik:RadButton x:Name="input6_Button" Content="6" Grid.Row="3" Grid.Column="2" Command="{x:Static controls:RadCalculatorCommands.UpdateInput}" CommandParameter="6"/>
