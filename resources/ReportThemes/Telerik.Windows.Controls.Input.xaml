                        <telerik:RadButton x:Name="ClearInput_Button" Content="CE" Grid.Row="1" Grid.Column="1" Command="{x:Static controls:RadCalculatorCommands.ClearInput}"/>
                        <telerik:RadButton x:Name="ClearEverything_Button" Content="C" Grid.Row="1" Grid.Column="2" Command="{x:Static controls:RadCalculatorCommands.ClearEverything}"/>
