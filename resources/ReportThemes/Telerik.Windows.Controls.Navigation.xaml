        <Setter Property="UseLayoutRounding" Value="True"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="IsHitTestVisible" Value="{Binding IsExpandable, RelativeSource={RelativeSource AncestorType=telerikNavigation:LayoutControlExpanderGroup}}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:RadToggleButton">
                    <Border x:Name="BorderVisual" Background="{TemplateBinding Background}" BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}">
                        <Grid x:Name="HeaderPanel" Background="Transparent" Margin="{TemplateBinding Padding}">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <ContentPresenter x:Name="HeaderContent"
                                    Content="{TemplateBinding Content}"
                                    ContentTemplate="{TemplateBinding ContentTemplate}"
                                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"/>
                            <Border x:Name="arrowPanel"
                                    Grid.Column="1"
                                    RenderTransformOrigin="0.5 0.5"
                                    Background="Transparent"
                                    BorderBrush="Transparent"
                                    BorderThickness="0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Visibility="{Binding IsExpandable, RelativeSource={RelativeSource AncestorType=telerikNavigation:LayoutControlExpanderGroup}, Converter={StaticResource BooleanToVisibilityConverter}}">
                                <Border.RenderTransform>
                                    <RotateTransform Angle="{Binding ExpandDirection, RelativeSource={RelativeSource AncestorType=telerik1:RadExpander},Converter={StaticResource ExpandDirectionToRotationAngleConverter}}"/>
                                </Border.RenderTransform>
                                <TextBlock x:Name="arrow" Tag="{TemplateBinding IsChecked}" Style="{Binding Tag, RelativeSource={RelativeSource TemplatedParent}}"/>
                            </Border>
                        </Grid>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="BorderVisual" Property="Background" Value="{telerik1:MaterialResource ResourceKey=AlternativeBrush}"/>
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{telerik1:MaterialResource ResourceKey=AlternativeBrush}"/>
                            <Setter TargetName="HeaderContent" Property="TextElement.Foreground" Value="{telerik1:MaterialResource ResourceKey=MarkerBrush}"/>
                            <Setter TargetName="HeaderContent" Property="Opacity" Value="{telerik1:MaterialResource ResourceKey=DisabledOpacity}"/>
                            <Setter TargetName="arrow" Property="Foreground" Value="{telerik1:MaterialResource ResourceKey=IconBrush}"/>
                            <Setter TargetName="arrow" Property="Opacity" Value="{telerik1:MaterialResource ResourceKey=DisabledOpacity}"/>
                        </Trigger>
                        <DataTrigger Binding="{Binding IsExpandable, RelativeSource={RelativeSource AncestorType=telerikNavigation:LayoutControlExpanderGroup}}" Value="False">
                            <Setter Property="MinHeight" Value="48"/>
                        </DataTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="LayoutControlToolBoxExpanderHeaderButton" TargetType="telerik:RadToggleButton" BasedOn="{StaticResource LayoutControlExpanderHeaderButtonStyle}">
        <Setter Property="Tag" Value="{StaticResource LayoutControlToolBoxExpanderButtonIconStyle}"/>
        <Setter Property="Background" Value="{telerik1:MaterialResource ResourceKey=MainBrush}"/>
        <Setter Property="Foreground" Value="{telerik1:MaterialResource ResourceKey=MarkerBrush}"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="Padding" Value="16"/>
        <Style.Triggers>
            <Trigger Property="IsChecked" Value="True">
                <Setter Property="Foreground" Value="{telerik1:MaterialResource ResourceKey=PrimaryNormalBrush}"/>
            </Trigger>
        </Style.Triggers>
    </Style>
    <Style x:Key="LayoutControlSplitterStyle" TargetType="layoutControl:LayoutControlSplitter">
        <Setter Property="MinWidth" Value="14"/>
        <Setter Property="MinHeight" Value="14"/>
        <Setter Property="MaxWidth" Value="14"/>
        <Setter Property="Background" Value="{telerik1:MaterialResource ResourceKey=DividerBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik1:MaterialResource ResourceKey=DividerBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="UseLayoutRounding" Value="True"/>
        <Setter Property="Foreground" Value="{telerik1:MaterialResource ResourceKey=IconBrush}"/>
        <Setter Property="FontSize" Value="16"/>
        <Setter Property="Cursor" Value="SizeWE"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="layoutControl:LayoutControlSplitter">
                    <Border BorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}" BorderThickness="{TemplateBinding BorderThickness}">
                        <TextBlock
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Foreground="{TemplateBinding Foreground}"
                                Margin="{TemplateBinding Padding}"
                                telerik:GlyphAdorner.ShowGlyphInDesignTime="True"
                                FontSize="{TemplateBinding FontSize}"
                                FontWeight="Normal"
                                FontStyle="Normal"
                                FontFamily="{StaticResource TelerikWebUI}">
                            <Run x:Name="grip" Text="{StaticResource GlyphMoreVertical}"/>
                        </TextBlock>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="HorizontalAlignment" Value="Stretch">
                            <Setter TargetName="grip" Property="Text" Value="{StaticResource GlyphMoreHorizontal}"/>
                            <Setter Property="Cursor" Value="SizeNS"/>
                            <Setter Property="MaxWidth" Value="{x:Static sys:Double.MaxValue}"/>
                            <Setter Property="MaxHeight" Value="14"/>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter Property="Opacity" Value="{telerik1:MaterialResource ResourceKey=DisabledOpacity}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="LayoutControlToolBoxWindowStyle" TargetType="telerikNavigation:RadWindow">
        <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth1"/>
        <Setter Property="IsTopmost" Value="True"/>
        <Setter Property="Header" Value="Customize"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="CaptionHeight" Value="48"/>
        <Setter Property="Padding" Value="9 0 9 9"/>
        <Setter Property="Background" Value="{telerik1:MaterialResource ResourceKey=MainBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik1:MaterialResource ResourceKey=DividerBrush}"/>
        <Setter Property="Foreground" Value="{telerik1:MaterialResource ResourceKey=MarkerInvertedBrush}"/>
        <Setter Property="FontSize" Value="{telerik1:MaterialResource ResourceKey=FontSize}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikNavigation:RadWindow">
                    <Grid Margin="3">
                        <mat:Shadow ShadowDepth="{TemplateBinding mat:MaterialAssist.ShadowDepth}" Background="{TemplateBinding Background}"/>
                        <Border x:Name="WindowOuterBorder"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                Background="{TemplateBinding Background}">
                            <AdornerDecorator>
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="*"/>
                                    </Grid.RowDefinitions>
                                    <Border x:Name="Header"
                                            Background="{telerik1:MaterialResource ResourceKey=PrimaryNormalBrush}"
                                            BorderBrush="{telerik1:MaterialResource ResourceKey=PrimaryNormalBrush}"
                                            BorderThickness="0">
                                        <DockPanel HorizontalAlignment="Stretch" Margin="24 0 0 0">
                                            <ContentControl x:Name="HeaderContent"
                                                    IsTabStop="False"
                                                    HorizontalAlignment="Stretch"
                                                    VerticalAlignment="Center"
                                                    HorizontalContentAlignment="Stretch"
                                                    Content="{TemplateBinding Header}"
                                                    SnapsToDevicePixels="True"
                                                    Foreground="{TemplateBinding Foreground}"
                                                    FontSize="{TemplateBinding FontSize}"
                                                    FontFamily="{TemplateBinding FontFamily}"/>
                                            <telerik:RadButton x:Name="PART_CloseButton"
                                                    Command="{x:Static telerik:WindowCommands.Close}"
                                                    Content="{StaticResource GlyphClose}"
                                                    CornerRadius="0"
                                                    mat:MaterialAssist.MouseOverBrush="{telerik1:MaterialResource ResourceKey=PrimaryHoverBrush}"
                                                    mat:MaterialAssist.PressedBrush="{telerik1:MaterialResource ResourceKey=PrimaryPressedBrush}"
                                                    FontFamily="{StaticResource TelerikWebUI}"
                                                    Foreground="{telerik1:MaterialResource ResourceKey=MarkerInvertedBrush}"
                                                    FontSize="16"
                                                    FontWeight="Normal"
                                                    FontStyle="Normal"
                                                    Padding="16"
                                                    DockPanel.Dock="Right"
                                                    HorizontalAlignment="Right"
                                                    Background="Transparent"
                                                    BorderBrush="Transparent"/>
                                        </DockPanel>
                                    </Border>
                                    <Border x:Name="ContentOuterBorder" Grid.Row="1" Background="{x:Null}" BorderBrush="{TemplateBinding BorderBrush}">
                                        <ContentPresenter x:Name="ContentElement" Margin="{TemplateBinding Padding}" Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}"/>
                                    </Border>
                                </Grid>
                            </AdornerDecorator>
                        </Border>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="LayoutControlToolBoxStyle" TargetType="layoutControl:LayoutControlToolBox">
        <Setter Property="Canvas.Right" Value="0"/>
        <Setter Property="Panel.ZIndex" Value="100"/>
        <Setter Property="Margin" Value="5"/>
        <Setter Property="Padding" Value="10"/>
        <Setter Property="WindowStyle" Value="{StaticResource LayoutControlToolBoxWindowStyle}"/>
        <Setter Property="ToolBoxView">
            <Setter.Value>
                <layoutControl:LayoutControlToolBoxView/>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="layoutControl:LayoutControlToolBox">
                    <Grid>
                        <telerik:RadToggleButton
                                IsChecked="{Binding IsOpen, RelativeSource={RelativeSource TemplatedParent}}"
                                Content="{StaticResource GlyphGear}"
                                Padding="{TemplateBinding Padding}"
                                FontWeight="Normal"
                                FontStyle="Normal"
                                FontFamily="{StaticResource TelerikWebUI}"
                                FontSize="16"
                                Focusable="False"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="telerikNavigation:RadTreeViewItem" x:Key="LayoutControlStructureTabTreeViewItem" BasedOn="{StaticResource RadTreeViewItemStyle}">
        <Setter Property="IsExpanded" Value="True"/>
        <Setter Property="Background" Value="#01FFFFFF"/>
        <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
        <Setter Property="Padding" Value="0"/>
    </Style>
    <HierarchicalDataTemplate x:Key="StructureItemHierarchicalDataTemplate" ItemsSource="{Binding Items}">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Path
                    VerticalAlignment="Center"
                    Data="{Binding OriginalItemType, Converter={StaticResource TypeToPathDataConverter}}"
                    Opacity="{telerik1:MaterialResource ResourceKey=SecondaryOpacity}"
                    Fill="{Binding Foreground, ElementName=HeaderText}"
                    Margin="10"/>
            <TextBlock x:Name="HeaderText"
                    Margin="12 0 0 0"
                    VerticalAlignment="Center"
                    Text="{Binding Header}"
                    Grid.Column="1"
                    Opacity="{telerik1:MaterialResource ResourceKey=PrimaryOpacity}"/>
        </Grid>
    </HierarchicalDataTemplate>
    <Style TargetType="layoutControl:LayoutControlToolBoxView" x:Key="LayoutControlToolBoxViewStyle">
        <Setter Property="telerik:DragDropManager.AllowDrag" Value="True"/>
        <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth1"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="layoutControl:LayoutControlToolBoxView">
                    <Grid>
                        <telerikNavigation:LayoutControlTabGroup Grid.Row="1" Background="{TemplateBinding Background}" Padding="1">
                            <telerikNavigation:LayoutControlTabGroupItem Header="Items">
                                <StackPanel Orientation="Vertical">
                                    <Grid Margin="3">
                                        <mat:Shadow ShadowDepth="{TemplateBinding mat:MaterialAssist.ShadowDepth}" Background="{telerik1:MaterialResource ResourceKey=MainBrush}"/>
                                        <telerik:RadExpander Header="New Items" IsExpanded="True" HeaderButtonStyle="{StaticResource LayoutControlToolBoxExpanderHeaderButton}" Padding="0">
                                            <telerik:RadListBox
                                                    ItemsSource="{Binding NewItems, RelativeSource={RelativeSource TemplatedParent}}"
                                                    BorderThickness="0"
                                                    mat:MaterialAssist.ShadowDepth="Depth0"
                                                    ItemTemplate="{StaticResource StructureItemHierarchicalDataTemplate}"
                                                    ScrollViewer.VerticalScrollBarVisibility="Disabled"/>
                                        </telerik:RadExpander>
                                    </Grid>
                                    <Grid Margin="3">
                                        <mat:Shadow ShadowDepth="{TemplateBinding mat:MaterialAssist.ShadowDepth}" Background="{telerik1:MaterialResource ResourceKey=MainBrush}"/>
                                        <telerik:RadExpander
                                                Header="Deleted Items"
                                                IsExpanded="True"
                                                HeaderButtonStyle="{StaticResource LayoutControlToolBoxExpanderHeaderButton}"
                                                Padding="0"
                                                Visibility="{TemplateBinding TrackDeletedItems,Converter={StaticResource BooleanToVisibilityConverter}}">
                                            <telerik:RadListBox x:Name="deletedItemsList"
                                                    ItemsSource="{Binding DeletedItems, RelativeSource={RelativeSource TemplatedParent}}"
                                                    BorderThickness="0"
                                                    ItemTemplate="{StaticResource StructureItemHierarchicalDataTemplate}"
                                                    ScrollViewer.VerticalScrollBarVisibility="Disabled"
                                                    mat:MaterialAssist.ShadowDepth="Depth0"
                                                    Visibility="{Binding HasItems, RelativeSource={RelativeSource Self}, Converter={StaticResource BooleanToVisibilityConverter}}"/>
                                            <telerik:RadExpander.Style>
                                                <Style TargetType="telerik:RadExpander" BasedOn="{StaticResource RadExpanderStyle}">
                                                    <Style.Triggers>
                                                        <DataTrigger Binding="{Binding Path=Items.Count, ElementName=deletedItemsList}" Value="0">
                                                            <Setter Property="IsEnabled" Value="False"/>
                                                        </DataTrigger>
                                                    </Style.Triggers>
                                                </Style>
                                            </telerik:RadExpander.Style>
                                        </telerik:RadExpander>
                                    </Grid>
                                </StackPanel>
                            </telerikNavigation:LayoutControlTabGroupItem>
                            <telerikNavigation:LayoutControlTabGroupItem Header="Structure">
                                <telerikNavigation:RadTreeView x:Name="PART_StructureTree" ItemTemplate="{StaticResource StructureItemHierarchicalDataTemplate}" ItemContainerStyle="{StaticResource LayoutControlStructureTabTreeViewItem}"/>
                            </telerikNavigation:LayoutControlTabGroupItem>
                        </telerikNavigation:LayoutControlTabGroup>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="LayoutControlResizerStyle" TargetType="{x:Type Thumb}">
        <Setter Property="Background" Value="#01FFFFFF"/>
        <Setter Property="MinHeight" Value="11"/>
        <Setter Property="MinWidth" Value="11"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Thumb}">
                    <Border
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            Background="{TemplateBinding Background}"
                            SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="LayoutControlSelectionIndicatorStyle" TargetType="layoutControl:LayoutControlSelectionIndicator">
        <Setter Property="Background" Value="{telerik1:MaterialResource ResourceKey=PrimaryFocusBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik1:MaterialResource ResourceKey=PrimaryFocusBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="layoutControl:LayoutControlSelectionIndicator">
                    <Grid x:Name="GridRoot">
                        <Border BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}">
                            <Grid>
                                <Border Background="{TemplateBinding Background}" Opacity="0.5"/>
                                <Grid x:Name="LockPathGrid" Opacity="0" Width="64" Height="64">
                                    <Ellipse StrokeThickness="2" Stroke="{telerik1:MaterialResource ResourceKey=MainBrush}"/>
                                    <TextBlock x:Name="glyph"
                                            VerticalAlignment="Center"
                                            FontSize="32"
                                            FontWeight="Normal"
                                            FontStyle="Normal"
                                            telerik:GlyphAdorner.ShowGlyphInDesignTime="True"
                                            HorizontalAlignment="Center"
                                            FontFamily="{StaticResource TelerikWebUI}"
                                            Foreground="{telerik1:MaterialResource ResourceKey=MarkerInvertedBrush}">
                                        <Run Text="{StaticResource GlyphLock}"/>
                                    </TextBlock>
                                </Grid>
                            </Grid>
                        </Border>
                        <Thumb x:Name="PART_TopResizer" Margin="0 -5 0 0" HorizontalAlignment="Stretch" VerticalAlignment="Top" Style="{StaticResource LayoutControlResizerStyle}" Cursor="SizeNS"/>
                        <Thumb x:Name="PART_LeftResizer" Margin="-5 0 0 0" HorizontalAlignment="Left" VerticalAlignment="Stretch" Style="{StaticResource LayoutControlResizerStyle}" Cursor="SizeWE"/>
                        <Thumb x:Name="PART_RightResizer"
                                Margin="0 0 -5 0"
                                HorizontalAlignment="Right"
                                VerticalAlignment="Stretch"
                                Style="{StaticResource LayoutControlResizerStyle}"
                                Cursor="SizeWE"/>
                        <Thumb x:Name="PART_BottomResizer"
                                Margin="0 0 0 -5"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Bottom"
                                Style="{StaticResource LayoutControlResizerStyle}"
                                Cursor="SizeNS"/>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsContextItemLocked" Value="True">
                            <Setter TargetName="LockPathGrid" Property="Opacity" Value="1"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="LayoutControlMouseOverIndicatorStyle" TargetType="layoutControl:LayoutControlMouseOverIndicator">
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="BorderBrush" Value="{telerik1:MaterialResource ResourceKey=PrimaryNormalBrush}"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate>
                    <Border
                            Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="LayoutControlDragVisualStyle" TargetType="layoutControl:LayoutControlDragVisual">
        <Setter Property="BorderBrush" Value="{telerik1:MaterialResource ResourceKey=PrimaryNormalBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="Width" Value="{Binding ContextItem.ActualWidth, RelativeSource={RelativeSource Self}}"/>
        <Setter Property="Height" Value="{Binding ContextItem.ActualHeight, RelativeSource={RelativeSource Self}}"/>
        <Setter Property="Background" Value="{Binding ContextItem, RelativeSource={RelativeSource Self}, Converter={StaticResource VisualToVisualBrushConverter}}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="layoutControl:LayoutControlDragVisual">
                    <Border
                            Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="LayoutControlDropIndicatorStyle" TargetType="layoutControl:LayoutControlDropIndicator">
        <Setter Property="MinWidth" Value="8"/>
        <Setter Property="MinHeight" Value="8"/>
        <Setter Property="BorderBrush" Value="{telerik1:MaterialResource ResourceKey=PrimaryNormalBrush}"/>
        <Setter Property="Background" Value="{telerik1:MaterialResource ResourceKey=PrimaryNormalBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="layoutControl:LayoutControlDropIndicator">
                    <Border x:Name="BorderVisual"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}">
                        <Border x:Name="OuterBorderVisual" Background="{TemplateBinding Background}" Opacity="0.25" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"/>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsDropTarget" Value="True">
                            <Setter TargetName="OuterBorderVisual" Property="Opacity" Value="0.5"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <ControlTemplate x:Key="LayoutControlTabGroupControlTemplate" TargetType="telerikNavigation:LayoutControlTabGroup">
        <Grid Background="#01FFFFFF">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <ItemsPresenter/>
            <ScrollViewer
                    Grid.Row="1"
                    Padding="{TemplateBinding Padding}"
                    Background="{TemplateBinding Background}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    HorizontalScrollBarVisibility="{TemplateBinding ScrollViewer.HorizontalScrollBarVisibility}"
                    VerticalScrollBarVisibility="{TemplateBinding ScrollViewer.VerticalScrollBarVisibility}">
                <ContentControl x:Name="PART_ContentElement" Grid.Row="1" Content="{TemplateBinding SelectedItem}"/>
            </ScrollViewer>
        </Grid>
    </ControlTemplate>
    <ControlTemplate x:Key="LayoutControlExpanderGroupControlTemplate" TargetType="telerikNavigation:LayoutControlExpanderGroup">
        <Grid Margin="3">
            <mat:Shadow ShadowDepth="{TemplateBinding mat:MaterialAssist.ShadowDepth}" Background="{telerik1:MaterialResource ResourceKey=MainBrush}"/>
            <telerik:RadExpander
                    Header="{TemplateBinding Header}"
                    Padding="{TemplateBinding Padding}"
                    Background="{TemplateBinding Background}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    animation:AnimationManager.IsAnimationEnabled="{Binding Path=(animation:AnimationManager.IsAnimationEnabled), RelativeSource={RelativeSource TemplatedParent}}"
                    IsExpanded="{Binding IsExpanded, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"
                    HeaderButtonStyle="{StaticResource LayoutControlExpanderHeaderButtonStyle}">
                <ScrollViewer
                        BorderThickness="0"
                        Background="Transparent"
                        HorizontalScrollBarVisibility="{TemplateBinding ScrollViewer.HorizontalScrollBarVisibility}"
                        VerticalScrollBarVisibility="{TemplateBinding ScrollViewer.VerticalScrollBarVisibility}">
                    <ItemsPresenter/>
                </ScrollViewer>
            </telerik:RadExpander>
        </Grid>
    </ControlTemplate>
    <ControlTemplate x:Key="LayoutControlGroupControlTemplate" TargetType="telerikNavigation:LayoutControlGroup">
        <ScrollViewer
                HorizontalScrollBarVisibility="{TemplateBinding ScrollViewer.HorizontalScrollBarVisibility}"
                VerticalScrollBarVisibility="{TemplateBinding ScrollViewer.VerticalScrollBarVisibility}"
                Padding="{TemplateBinding Padding}"
                Background="{TemplateBinding Background}"
                BorderBrush="{TemplateBinding BorderBrush}"
                BorderThickness="{TemplateBinding BorderThickness}">
            <ItemsPresenter/>
        </ScrollViewer>
    </ControlTemplate>
    <Style x:Key="LayoutControlTabGroupItemContentStyle" TargetType="telerikNavigation:LayoutControlTabGroupItem">
        <Setter Property="Header" Value="Default Header"/>
        <Setter Property="VerticalAlignment" Value="Stretch"/>
        <Setter Property="HorizontalAlignment" Value="Stretch"/>
        <Setter Property="BorderBrush" Value="{telerik1:MaterialResource ResourceKey=DividerBrush}"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="Foreground" Value="{telerik1:MaterialResource ResourceKey=MarkerBrush}"/>
        <Setter Property="FontFamily" Value="{telerik1:MaterialResource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik1:MaterialResource ResourceKey=FontSize}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikNavigation:LayoutControlTabGroupItem">
                    <Border BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}">
                        <ContentPresenter/>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="LayoutControlTabGroupItemStyle" TargetType="telerikNavigation:LayoutControlTabGroupItem">
        <Setter Property="Header" Value="Default Header"/>
        <Setter Property="VerticalAlignment" Value="Top"/>
        <Setter Property="HorizontalAlignment" Value="Left"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="MinWidth" Value="48"/>
        <Setter Property="MinHeight" Value="48"/>
        <Setter Property="Padding" Value="25 13"/>
        <Setter Property="mat:MaterialAssist.MouseOverBrush" Value="{telerik1:MaterialResource ResourceKey=AlternativeBrush}"/>
        <Setter Property="mat:MaterialAssist.PressedBrush" Value="{telerik1:MaterialResource ResourceKey=AlternativeBrush}"/>
        <Setter Property="mat:MaterialAssist.FocusBrush" Value="{telerik1:MaterialResource ResourceKey=AccentNormalBrush}"/>
        <Setter Property="Foreground" Value="{telerik1:MaterialResource ResourceKey=MarkerBrush}"/>
        <Setter Property="FontFamily" Value="{telerik1:MaterialResource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik1:MaterialResource ResourceKey=FontSize}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikNavigation:LayoutControlTabGroupItem">
                    <Grid x:Name="wrapper">
                        <Border x:Name="BorderVisual" BorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}" BorderThickness="{TemplateBinding BorderThickness}"/>
                        <mat:MaterialControl IsSmartClipped="True">
                            <ContentControl x:Name="HeaderElement"
                                    IsTabStop="False"
                                    Opacity="{telerik1:MaterialResource ResourceKey=PrimaryOpacity}"
                                    Content="{TemplateBinding Header}"
                                    ContentTemplate="{TemplateBinding HeaderTemplate}"
                                    Foreground="{TemplateBinding Foreground}"
                                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                    Margin="{TemplateBinding Padding}"
                                    ContentStringFormat="{TemplateBinding HeaderStringFormat}"
                                    ContentTemplateSelector="{TemplateBinding HeaderTemplateSelector}"
                                    SnapsToDevicePixels="True"/>
                        </mat:MaterialControl>
                        <Rectangle x:Name="SelectedVisualNoAnimation"
                                VerticalAlignment="Bottom"
                                HorizontalAlignment="Stretch"
                                Fill="{TemplateBinding mat:MaterialAssist.FocusBrush}"
                                Height="3"
                                Visibility="Collapsed"/>
                        <Border x:Name="SelectedVisual"
                                BorderBrush="{TemplateBinding mat:MaterialAssist.FocusBrush}"
                                BorderThickness="0 0 0 3"
                                VerticalAlignment="Stretch"
                                HorizontalAlignment="Stretch"
                                RenderTransformOrigin="0.5 1">
                            <Border.RenderTransform>
                                <ScaleTransform ScaleX="0" ScaleY="1"/>
                            </Border.RenderTransform>
                        </Border>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsSelected" Value="True"/>
                                <Condition Property="animation:AnimationManager.IsAnimationEnabled" Value="False"/>
                            </MultiTrigger.Conditions>
                            <Setter TargetName="SelectedVisualNoAnimation" Property="Visibility" Value="Visible"/>
                        </MultiTrigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsSelected" Value="True"/>
                                <Condition Property="animation:AnimationManager.IsAnimationEnabled" Value="True"/>
                            </MultiTrigger.Conditions>
                            <MultiTrigger.EnterActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="RenderTransform.ScaleX" Storyboard.TargetName="SelectedVisual">
                                            <EasingDoubleKeyFrame KeyTime="0" Value="0"/>
                                            <EasingDoubleKeyFrame KeyTime="0:0:0.12" Value="1">
                                                <EasingDoubleKeyFrame.EasingFunction>
                                                    <SineEase EasingMode="EaseIn"/>
                                                </EasingDoubleKeyFrame.EasingFunction>
                                            </EasingDoubleKeyFrame>
                                        </DoubleAnimationUsingKeyFrames>
                                    </Storyboard>
                                </BeginStoryboard>
                            </MultiTrigger.EnterActions>
                            <MultiTrigger.ExitActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="RenderTransform.ScaleX" Storyboard.TargetName="SelectedVisual">
                                            <EasingDoubleKeyFrame KeyTime="0" Value="1"/>
                                            <EasingDoubleKeyFrame KeyTime="0:0:0.1" Value="0">
                                                <EasingDoubleKeyFrame.EasingFunction>
                                                    <SineEase EasingMode="EaseIn"/>
                                                </EasingDoubleKeyFrame.EasingFunction>
                                            </EasingDoubleKeyFrame>
                                        </DoubleAnimationUsingKeyFrames>
                                    </Storyboard>
                                </BeginStoryboard>
                            </MultiTrigger.ExitActions>
                        </MultiTrigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="HeaderElement" Property="Opacity" Value="{telerik1:MaterialResource ResourceKey=DisabledOpacity}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
