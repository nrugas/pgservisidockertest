    <DataTemplate x:Key="RadDesktopAlertHeaderTemplate">
        <Grid x:Name="Header" VerticalAlignment="Top" Grid.Row="0" Grid.Column="1">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>
            <StackPanel
                    Orientation="Horizontal"
                    VerticalAlignment="Top"
                    HorizontalAlignment="Right"
                    DataContext="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=telerikNavigation:RadDesktopAlert}}">
                <telerik:RadDropDownButton x:Name="DesktopAlert_DropDownButton"
                        IsOpen="{Binding IsMenuOpen, Mode=TwoWay}"
                        Style="{StaticResource DesktopAlertDropDownButtonStyle}"
                        Visibility="{Binding ShowMenuButton, Converter={StaticResource BooleanToVisibilityConverter}}">
                    <telerik:RadDropDownButton.DropDownContent>
                        <telerikNavigation:RadContextMenu ItemsSource="{Binding MenuItemsSource}" ItemContainerStyle="{Binding MenuItemContainerStyle}"/>
                    </telerik:RadDropDownButton.DropDownContent>
                </telerik:RadDropDownButton>
                <telerik:RadButton x:Name="DesktopAlert_CloseButton"
                        Command="{x:Static telerik:DesktopAlertCommands.Close}"
                        Style="{StaticResource DesktopAlertCloseButtonStyle}"
                        Visibility="{Binding ShowCloseButton, Converter={StaticResource BooleanToVisibilityConverter}}">
                    <telerik:RadButton.Content>
                        <TextBlock x:Name="Content"
                                telerik:GlyphAdorner.ShowGlyphInDesignTime="True"
                                FontSize="16"
                                FontStyle="Normal"
                                FontWeight="Normal"
                                FontFamily="{StaticResource TelerikWebUI}"
                                Foreground="{Binding RelativeSource={RelativeSource AncestorType={x:Type telerik:RadButton}}, Path=Foreground}"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center">
                            <Run Text="{StaticResource GlyphClose}"/>
                        </TextBlock>
                    </telerik:RadButton.Content>
                    <ToolTipService.ToolTip>
                        <ToolTip telerik:LocalizationManager.ResourceKey="Close"/>
                    </ToolTipService.ToolTip>
                </telerik:RadButton>
            </StackPanel>
            <ContentPresenter
                    Content="{Binding}"
                    Grid.RowSpan="2"
                    VerticalAlignment="Center"
                    TextBlock.FontWeight="Bold"
                    MinHeight="{Binding ActualHeight, ElementName=DesktopAlert_CloseButton}"
                    Margin="12 12 70 0">
                <ContentPresenter.Resources>
                    <Style TargetType="TextBlock">
                        <Setter Property="TextWrapping" Value="Wrap"/>
                    </Style>
                </ContentPresenter.Resources>
            </ContentPresenter>
        </Grid>
    </DataTemplate>
    <ControlTemplate x:Key="RadDesktopAlertTemplate" TargetType="telerikNavigation:RadDesktopAlert">
        <Grid Margin="3">
            <mat:Shadow Background="{TemplateBinding Background}" ShadowDepth="{TemplateBinding mat:MaterialAssist.ShadowDepth}"/>
            <Border x:Name="BorderVisual"
                    Background="{TemplateBinding Background}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}">
                <Grid>
                    <Rectangle x:Name="MouseOverVisual" Fill="{TemplateBinding mat:MaterialAssist.MouseOverBrush}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Opacity="0"/>
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <ContentPresenter
                                Content="{TemplateBinding Header}"
                                ContentTemplate="{TemplateBinding HeaderTemplate}"
                                Grid.Row="0"
                                Grid.Column="1"
                                VerticalAlignment="Top"
                                TextBlock.Foreground="{TemplateBinding Foreground}"/>
                        <ContentPresenter x:Name="Icon"
                                Grid.Column="0"
                                Grid.RowSpan="2"
                                Content="{TemplateBinding Icon}"
                                Width="{TemplateBinding IconColumnWidth}"
                                Margin="{TemplateBinding IconMargin}"
                                VerticalAlignment="Center"
                                HorizontalAlignment="Center"
                                ContentTemplate="{TemplateBinding IconTemplate}"/>
                        <ContentPresenter x:Name="Content"
                                Grid.Row="1"
                                Grid.Column="1"
                                Margin="{TemplateBinding Padding}"
                                TextBlock.Foreground="{TemplateBinding Foreground}"
                                telerikNavigation:WindowHost.HitTestable="True"
                                VerticalAlignment="Top"
                                HorizontalAlignment="Left"
                                Cursor="Hand">
                            <ContentPresenter.Resources>
                                <Style TargetType="TextBlock">
                                    <Setter Property="TextWrapping" Value="Wrap"/>
                                    <Setter Property="TextTrimming" Value="CharacterEllipsis"/>
                                    <Style.Triggers>
                                        <Trigger Property="TextBlock.IsMouseOver" Value="True">
                                            <Setter Property="TextDecorations" Value="Underline"/>
                                        </Trigger>
                                    </Style.Triggers>
                                </Style>
                            </ContentPresenter.Resources>
                        </ContentPresenter>
                    </Grid>
                </Grid>
            </Border>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOverAlert" Value="True">
                <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth2"/>
            </Trigger>

            <!-- IsMouseOver and animation disabled -->
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="animation:AnimationManager.IsAnimationEnabled" Value="False"/>
                    <Condition Property="IsMouseOverAlert" Value="True"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="BorderVisual" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(mat:MaterialAssist.MouseOverBrush), Mode=OneWay}"/>
            </MultiTrigger>

            <!-- IsMouseOver and animation enabled -->
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="animation:AnimationManager.IsAnimationEnabled" Value="True"/>
                    <Condition Property="IsMouseOverAlert" Value="True"/>
                </MultiTrigger.Conditions>
                <MultiTrigger.EnterActions>
                    <BeginStoryboard>
                        <Storyboard>
                            <DoubleAnimation Duration="00:00:00.20" From="0" To="1" Storyboard.TargetName="MouseOverVisual" Storyboard.TargetProperty="Opacity"/>
                        </Storyboard>
                    </BeginStoryboard>
                </MultiTrigger.EnterActions>
                <MultiTrigger.ExitActions>
                    <BeginStoryboard>
                        <Storyboard>
                            <DoubleAnimation Duration="00:00:00.20" From="1" To="0" Storyboard.TargetName="MouseOverVisual" Storyboard.TargetProperty="Opacity"/>
                        </Storyboard>
                    </BeginStoryboard>
                </MultiTrigger.ExitActions>
            </MultiTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="DesktopAlertMenuItemContainerStyle" TargetType="telerikNavigation:RadMenuItem" BasedOn="{StaticResource RadMenuItemStyle}">
        <Setter Property="Header" Value="{Binding Header}"/>
        <Setter Property="Command" Value="{Binding Command}"/>
        <Setter Property="CommandParameter" Value="{Binding CommandParameter}"/>
        <Setter Property="ItemsSource" Value="{Binding ItemsSource}"/>
        <Setter Property="IsSeparator" Value="{Binding IsSeparator}"/>
        <Setter Property="IsCheckable" Value="{Binding IsCheckable}"/>
        <Setter Property="IsChecked" Value="{Binding IsChecked}"/>
        <Setter Property="Icon" Value="{Binding IconUrl}"/>
        <Setter Property="IconTemplate">
            <Setter.Value>
                <DataTemplate>
                    <Image Source="{Binding}" Stretch="None"/>
                </DataTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadDesktopAlertStyle" TargetType="telerikNavigation:RadDesktopAlert">
        <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth1"/>
        <Setter Property="mat:MaterialAssist.MouseOverBrush" Value="{telerik1:MaterialResource ResourceKey=AlternativeBrush}"/>
        <Setter Property="FontFamily" Value="{telerik1:MaterialResource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik1:MaterialResource ResourceKey=FontSize}"/>
        <Setter Property="Foreground" Value="{telerik1:MaterialResource ResourceKey=MarkerBrush}"/>
        <Setter Property="Background" Value="{telerik1:MaterialResource ResourceKey=MainBrush}"/>
        <Setter Property="Padding" Value="16 16 15 16"/>
        <Setter Property="Opacity" Value="1"/>
        <Setter Property="Width" Value="450"/>
        <Setter Property="Height" Value="130"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="HeaderTemplate" Value="{StaticResource RadDesktopAlertHeaderTemplate}"/>
        <Setter Property="MenuItemContainerStyle" Value="{StaticResource DesktopAlertMenuItemContainerStyle}"/>
        <Setter Property="Template" Value="{StaticResource RadDesktopAlertTemplate}"/>
    </Style>
    <Style TargetType="telerikNavigation:RadDesktopAlert" BasedOn="{StaticResource RadDesktopAlertStyle}"/>
</ResourceDictionary>
