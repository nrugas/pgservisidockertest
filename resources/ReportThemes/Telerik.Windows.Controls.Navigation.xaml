    <Style x:Key="BreadcrumbTextSplitButton" TargetType="telerik:RadSplitButton" BasedOn="{StaticResource RadSplitButtonStyle}">
        <Setter Property="CornerRadius" Value="0"/>
        <Setter Property="TogglePartStyle" Value="{StaticResource BreadCrumbHistoryToggleButtonStyle}"/>
        <Setter Property="Foreground" Value="{telerik1:MaterialResource ResourceKey=MarkerBrush}"/>
        <Setter Property="Background" Value="{x:Null}"/>
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="Padding" Value="3"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="IsOpen" Value="False"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:RadSplitButton">
                    <Grid>
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <ContentControl x:Name="Content"
                                    Content="{TemplateBinding Content}"
                                    Foreground="{TemplateBinding Foreground}"
                                    VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
                                    HorizontalContentAlignment="Stretch"
                                    IsTabStop="False"
                                    ContentTemplate="{TemplateBinding ContentTemplate}"/>
                            <telerik:RadToggleButton x:Name="DropDownPart"
                                    Grid.Column="1"
                                    IsTabStop="False"
                                    Focusable="False"
                                    Visibility="{TemplateBinding DropDownIndicatorVisibility}"
                                    Style="{TemplateBinding TogglePartStyle}"
                                    BorderThickness="{TemplateBinding BorderThickness}"
                                    Foreground="{TemplateBinding Foreground}"
                                    IsChecked="{Binding Path=IsOpen, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}">
                                <TextBlock x:Name="DropDownIndicator"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        telerik:GlyphAdorner.ShowGlyphInDesignTime="True"
                                        Opacity="{telerik1:MaterialResource ResourceKey=SecondaryOpacity}"
                                        FontFamily="{StaticResource TelerikWebUI}"
                                        FontWeight="Normal"
                                        FontStyle="Normal"
                                        FontSize="{StaticResource BreadCrumbGlyphSize}"
                                        IsHitTestVisible="False">
                                    <Run Text="{StaticResource GlyphArrowOverflowDown}"/>
                                </TextBlock>
                            </telerik:RadToggleButton>
                        </Grid>
                        <Popup x:Name="DropDownPopup"
                                AllowsTransparency="True"
                                StaysOpen="False"
                                Focusable="False"
                                Placement="Bottom"
                                IsOpen="{Binding Path=IsOpen, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}">
                            <Grid Margin="8 0 8 8">
                                <mat:Shadow Background="{telerik1:MaterialResource ResourceKey=MainBrush}" CornerRadius="{telerik1:MaterialResource ResourceKey=CornerRadiusBottom}" ShadowDepth="Depth2"/>
                                <Border x:Name="DropDownPopupBorder"
                                        Background="{telerik1:MaterialResource ResourceKey=MainBrush}"
                                        BorderThickness="0 1 0 0"
                                        BorderBrush="{telerik1:MaterialResource ResourceKey=DividerBrush}"
                                        Width="{TemplateBinding DropDownWidth}"
                                        Height="{TemplateBinding DropDownHeight}"
                                        MaxWidth="{TemplateBinding DropDownMaxWidth}"
                                        MaxHeight="{TemplateBinding DropDownMaxHeight}"
                                        CornerRadius="{telerik1:MaterialResource ResourceKey=CornerRadiusBottom}">
                                    <ContentPresenter x:Name="DropDownPopupContent"
                                            Content="{TemplateBinding DropDownContent}"
                                            ContentTemplate="{TemplateBinding DropDownContentTemplate}"
                                            HorizontalAlignment="Stretch"
                                            VerticalAlignment="Stretch"
                                            DataContext="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=DataContext}"/>
                                </Border>
                            </Grid>
                        </Popup>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <ControlTemplate x:Key="BarItemControlTemplate" TargetType="telerikBreadcrumb:RadBreadcrumbBarItem">
        <Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="SplitButtonVisibility">
                    <VisualState x:Name="SplitButtonVisible"/>
                    <VisualState x:Name="SplitButtonHidden">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="SplitButton" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border x:Name="FocusVisual"
                    IsHitTestVisible="False"
                    Background="{TemplateBinding mat:MaterialAssist.FocusBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    BorderBrush="{x:Null}"
                    Visibility="Collapsed"/>
            <Border x:Name="headerContainer" BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}">
                <telerik:RadSplitButton x:Name="SplitButton"
                        Margin="0"
                        Foreground="{Binding Foreground, RelativeSource={RelativeSource TemplatedParent}}"
                        Content="{TemplateBinding Header}"
                        FontSize="{TemplateBinding FontSize}"
                        ContentTemplate="{TemplateBinding HeaderTemplate}"
                        IsOpen="{Binding Path=IsPopupOpen, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}"
                        Style="{TemplateBinding SplitButtonStyle}"
                        Command="{x:Static telerikBreadcrumb:RadBreadcrumbCommands.ClearSelectedItem}"
                        ContentTemplateSelector="{TemplateBinding HeaderTemplateSelector}">
                    <telerik:RadSplitButton.DropDownContent>
                        <ScrollViewer HorizontalScrollBarVisibility="Auto" MaxWidth="270" MinWidth="148" MaxHeight="250" Padding="0" Margin="0" BorderThickness="0" VerticalScrollBarVisibility="Auto">
                            <ItemsPresenter/>
                        </ScrollViewer>
                    </telerik:RadSplitButton.DropDownContent>
                </telerik:RadSplitButton>
            </Border>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="HasItems" Value="False">
                <Setter TargetName="SplitButton" Property="DropDownIndicatorVisibility" Value="Collapsed"/>
            </Trigger>
            <Trigger Property="HasItems" Value="True">
                <Setter TargetName="SplitButton" Property="DropDownIndicatorVisibility" Value="Visible"/>
            </Trigger>
            <Trigger Property="IsLinearMode" Value="True">
                <Setter TargetName="SplitButton" Property="DropDownIndicatorVisibility" Value="Collapsed"/>
            </Trigger>
            <Trigger Property="IsButtonPartHidden" Value="True">
                <Setter TargetName="SplitButton" Property="IsButtonPartVisible" Value="False"/>
            </Trigger>
            <Trigger Property="IsKeyboardFocusWithin" Value="True">
                <Setter TargetName="FocusVisual" Property="Visibility" Value="Visible"/>
            </Trigger>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter TargetName="FocusVisual" Property="Visibility" Value="Collapsed"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <ControlTemplate x:Key="DropDownItemControlTemplate" TargetType="telerikBreadcrumb:RadBreadcrumbBarItem">
        <Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="Mouse">
                    <VisualState x:Name="MouseEnter">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="BorderVisual" Storyboard.TargetProperty="Background">
                                <DiscreteObjectKeyFrame KeyTime="0" Value="{telerik1:MaterialResource ResourceKey=AlternativeBrush}"/>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="MouseLeave"/>
                </VisualStateGroup>
                <VisualStateGroup x:Name="Selection">
                    <VisualState x:Name="Select">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="contentControl" Storyboard.TargetProperty="FontWeight">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <FontWeight>Bold</FontWeight>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="BorderVisual" Storyboard.TargetProperty="Background"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="UnSelect"/>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border x:Name="BorderVisual" Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}"/>
            <Grid Background="Transparent">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="29"/>
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>
                <Image
                        Style="{StaticResource BreadcrumbImageStyle}"
                        Source="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Image}"
                        Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsIconVisible, Converter={StaticResource BooleanToVisibilityConverter}}"
                        Margin="-3 0 0 0"/>
                <ContentControl x:Name="contentControl"
                        Grid.Column="1"
                        VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                        FontSize="{TemplateBinding FontSize}"
                        Foreground="{TemplateBinding Foreground}"
                        Content="{TemplateBinding DropDownHeader}"
                        ContentTemplate="{TemplateBinding DropDownHeaderTemplate}"
                        Margin="{TemplateBinding Padding}"
                        ContentTemplateSelector="{TemplateBinding HeaderTemplateSelector}"/>
            </Grid>
        </Grid>
    </ControlTemplate>
    <Style x:Key="RadBreadcrumbStyle" TargetType="telerikNavigation:RadBreadcrumb">
        <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth1"/>
        <Setter Property="FontFamily" Value="{telerik1:MaterialResource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik1:MaterialResource ResourceKey=FontSize}"/>
        <Setter Property="Foreground" Value="{telerik1:MaterialResource ResourceKey=MarkerBrush}"/>
        <Setter Property="Background" Value="{telerik1:MaterialResource ResourceKey=MainBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik1:MaterialResource ResourceKey=MainBrush}"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="MinWidth" Value="150"/>
        <Setter Property="SplitButtonStyle" Value="{StaticResource BreadcrumbSplitButtonStyle}"/>
        <Setter Property="OverflowButtonStyle" Value="{StaticResource BreadcrumbOverflowSplitButtonStyle}"/>
        <Setter Property="DropDownItemControlTemplate" Value="{StaticResource DropDownItemControlTemplate}"/>
        <Setter Property="BarItemControlTemplate" Value="{StaticResource BarItemControlTemplate}"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikNavigation:RadBreadcrumb">
                    <Grid x:Name="outerGrid">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="36"/>
                        </Grid.ColumnDefinitions>
                        <mat:Shadow Grid.ColumnSpan="3" Background="{TemplateBinding Background}" ShadowDepth="{TemplateBinding mat:MaterialAssist.ShadowDepth}"/>
                        <Border x:Name="NormalVisual"
                                Grid.ColumnSpan="3"
                                Background="{TemplateBinding Background}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"/>
                        <Image x:Name="image"
                                Style="{StaticResource BreadcrumbImageStyle}"
                                Margin="12 0"
                                Source="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=CurrentContainer.Image}"
                                Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsCurrentIconVisible, Converter={StaticResource BooleanToVisibilityConverter}}"/>
                        <telerik:RadSplitButton x:Name="historySplitButton"
                                IsTabStop="False"
                                Style="{StaticResource BreadcrumbTextSplitButton}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                TogglePartStyle="{StaticResource BreadCrumbHistoryToggleButtonStyle}"
                                IsOpen="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsHistoryOpen, Mode=TwoWay}"
                                Grid.Column="1"
                                Grid.ColumnSpan="2">
                            <telerik:RadSplitButton.Content>
                                <TextBox x:Name="pathTextBox"
                                        BorderThickness="0"
                                        mat:MaterialAssist.FocusBrush="{x:Null}"
                                        IsTabStop="False"
                                        VerticalAlignment="Center"
                                        Opacity="0"
                                        MinHeight="20"
                                        FontSize="{TemplateBinding FontSize}"/>
                            </telerik:RadSplitButton.Content>
                            <telerik:RadSplitButton.DropDownContent>
                                <ListBox x:Name="HistoryListBox"
                                        Background="{telerik1:MaterialResource ResourceKey=MainBrush}"
                                        BorderThickness="0"
                                        mat:MaterialAssist.ShadowDepth="Depth0"
                                        mat:MaterialAssist.CornerRadius="{telerik1:MaterialResource ResourceKey=CornerRadiusBottom}"
                                        Foreground="{TemplateBinding Foreground}"
                                        ItemContainerStyle="{StaticResource BreadcrumbListBoxItemStyle}"
                                        ItemsSource="{TemplateBinding HistoryItems}"
                                        FontSize="{TemplateBinding FontSize}"
                                        HorizontalContentAlignment="Stretch"
                                        VerticalContentAlignment="Stretch"
                                        SelectedItem="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SelectedHistoryItem, Mode=TwoWay}"
                                        MinHeight="36"/>
                            </telerik:RadSplitButton.DropDownContent>
                        </telerik:RadSplitButton>
                        <telerikBreadcrumb:RadBreadcrumbBar x:Name="BreadcrumbBar"
                                Grid.Column="1"
                                Background="Transparent"
                                Foreground="{TemplateBinding Foreground}"
                                FontSize="{TemplateBinding FontSize}"
                                ItemTemplate="{TemplateBinding ItemTemplate}"
                                ItemTemplateSelector="{TemplateBinding ItemTemplateSelector}"/>
                        <Border x:Name="FocusVisual"
                                Grid.ColumnSpan="3"
                                Background="{x:Null}"
                                BorderBrush="{telerik1:MaterialResource ResourceKey=PrimaryFocusBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                Visibility="Collapsed"/>
                        <Popup x:Name="AutocompletePopup" AllowsTransparency="True" StaysOpen="False" Focusable="False" Placement="Bottom">
                            <Grid Margin="8 0 8 8">
                                <ListBox x:Name="AutoCompleteListBox"
                                        mat:MaterialAssist.ShadowDepth="Depth2"
                                        ItemsSource="{TemplateBinding AutocompleteItems}"
                                        FontSize="{TemplateBinding FontSize}"
                                        Foreground="{TemplateBinding Foreground}"
                                        MinHeight="36"
                                        mat:MaterialAssist.CornerRadius="{telerik1:MaterialResource ResourceKey=CornerRadiusBottom}"
                                        BorderThickness="0 1 0 0"
                                        Background="{telerik1:MaterialResource ResourceKey=MainBrush}"
                                        BorderBrush="{telerik1:MaterialResource ResourceKey=DividerBrush}"
                                        MaxWidth="650"
                                        MaxHeight="500"
                                        MinWidth="150"
                                        ItemContainerStyle="{StaticResource BreadcrumbListBoxItemStyle}"
                                        SelectedItem="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SelectedAutocompleteItem, Mode=TwoWay}"/>
                            </Grid>
                        </Popup>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsFocused" Value="True">
                            <Setter TargetName="FocusVisual" Property="Visibility" Value="Visible"/>
                        </Trigger>
                        <Trigger Property="IsTextModeEnabled" Value="False">
                            <Setter TargetName="historySplitButton" Property="Visibility" Value="Collapsed"/>
                        </Trigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsTextModeEnabled" Value="True"/>
                                <Condition Property="IsTextMode" Value="True"/>
                            </MultiTrigger.Conditions>
                            <Setter TargetName="BreadcrumbBar" Property="Visibility" Value="Collapsed"/>
                            <Setter TargetName="FocusVisual" Property="Visibility" Value="Visible"/>
                            <Setter TargetName="pathTextBox" Property="Opacity" Value="1"/>
                            <Setter TargetName="NormalVisual" Property="Background" Value="{telerik1:MaterialResource ResourceKey=MainBrush}"/>
                        </MultiTrigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsTextModeEnabled" Value="True"/>
                                <Condition Property="IsTextMode" Value="False"/>
                            </MultiTrigger.Conditions>
                            <Setter TargetName="BreadcrumbBar" Property="Visibility" Value="Visible"/>
                        </MultiTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadBreadcrumbBarStyle" TargetType="telerikBreadcrumb:RadBreadcrumbBar">
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <StackPanel Orientation="Horizontal"/>
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikBreadcrumb:RadBreadcrumbBar">
                    <Grid>
                        <Border Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}">
                            <ItemsPresenter x:Name="ItemsPresented" ClipToBounds="True"/>
                        </Border>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
