            <Border x:Name="CheckedVisual"
                    Visibility="Collapsed"
                    Background="{TemplateBinding mat:MaterialAssist.CheckedBrush}"
                    BorderBrush="{x:Null}"
                    BorderThickness="{TemplateBinding BorderThickness}"/>
            <mat:MaterialControl ClipToBounds="True">
                <telerik:RadDockPanel Margin="{TemplateBinding Padding}">
                    <TextBlock x:Name="GlyphPanel"
                            Margin="2 2 4 2"
                            FontFamily="{StaticResource TelerikWebUI}"
                            Foreground="{telerik1:MaterialResource ResourceKey=MarkerBrush}"
                            FontSize="16"
                            FontWeight="Normal"
                            FontStyle="Normal"
                            Visibility="Collapsed">
                        <Run Text="{StaticResource GlyphCheck}"/>
                    </TextBlock>
                    <ContentControl x:Name="Content"
                            Grid.Column="2"
                            telerik:RadDockPanel.Dock="Right"
