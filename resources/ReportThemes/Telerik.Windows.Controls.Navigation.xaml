            </Trigger>
            <Trigger Property="IsSelected" Value="True">
                <Setter TargetName="Header" Property="Foreground" Value="{telerik1:MaterialResource ResourceKey=PrimaryPressedBrush}"/>
            </Trigger>
            <Trigger Property="IsMouseOver" Value="True" SourceName="Ripple">
                <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth2"/>
