        <Setter Property="MinWidth" Value="0"/>
        <Setter Property="Width" Value="48"/>
        <Setter Property="Height" Value="48"/>
        <Setter Property="VerticalAlignment" Value="Top"/>
        <Setter Property="HorizontalAlignment" Value="Left"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="ContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <TextBlock
                            Text="{StaticResource GlyphGear}"
                            FontSize="32"
                            FontFamily="{StaticResource TelerikWebUI}"
                            FontWeight="Normal"
                            FontStyle="Normal"
                            Foreground="{Binding Foreground, RelativeSource={RelativeSource AncestorType={x:Type telerikRadialMenu:RadialMenuButton}}}"/>
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="BackContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <TextBlock
                            Text="{StaticResource GlyphArrowLeft}"
                            FontSize="32"
                            FontFamily="{StaticResource TelerikWebUI}"
                            FontWeight="Normal"
                            FontStyle="Normal"
                            Foreground="{Binding Foreground, RelativeSource={RelativeSource AncestorType={x:Type telerikRadialMenu:RadialMenuButton}}}"
                            Margin="0 4 0 0"/>
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRadialMenu:RadialMenuButton">
                    <Grid x:Name="RootGrid">
                        <Grid>
                            <mat:Shadow Background="{TemplateBinding Background}" ShadowDepth="{TemplateBinding mat:MaterialAssist.ShadowDepth}" CornerRadius="24"/>
                            <Ellipse x:Name="BackgroundGlyph"
                                    Fill="{TemplateBinding Background}"
                                    Stroke="{TemplateBinding BorderBrush}"
                                    StrokeThickness="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=BorderThickness.Left}"
                                    RenderTransformOrigin=".5 .5"
                                    Width="{TemplateBinding Width}"
                                    Height="{TemplateBinding Height}"/>
                            <ContentPresenter x:Name="NormalGlyph"
                                    RenderTransformOrigin="0.5 0.5"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Content="{TemplateBinding Content}"
                                    ContentTemplate="{TemplateBinding ContentTemplate}"
                                    Margin="0 -1 0 0"/>
                            <ContentPresenter x:Name="BackGlyph"
                                    HorizontalAlignment="Center"
                                    Margin="0 -3 0 0"
                                    ContentTemplate="{Binding Path=BackContentTemplate, RelativeSource={RelativeSource AncestorType={x:Type telerikRadialMenu:RadialMenuButton}}}"
                                    VerticalAlignment="Center"
                                    RenderTransformOrigin="0.5 0.5"
                                    Visibility="Collapsed"/>
                        </Grid>
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="NavigationStates">
                                <VisualState x:Name="Back">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="NormalGlyph" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Collapsed</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="BackGlyph" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Home">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="NormalGlyph" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="BackGlyph" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Collapsed</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter Property="Foreground" Value="{telerik:MaterialResource ResourceKey=PrimaryNormalBrush}"/>
                            <Setter TargetName="BackgroundGlyph" Property="Fill" Value="{telerik:MaterialResource ResourceKey=AlternativeBrush}"/>
                            <Setter TargetName="BackgroundGlyph" Property="Stroke" Value="{telerik:MaterialResource ResourceKey=AlternativeBrush}"/>
                            <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth2"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter Property="Foreground" Value="{telerik:MaterialResource ResourceKey=PrimaryNormalBrush}"/>
                            <Setter TargetName="BackgroundGlyph" Property="Fill" Value="{telerik:MaterialResource ResourceKey=ComplementaryBrush}"/>
                            <Setter TargetName="BackgroundGlyph" Property="Stroke" Value="{telerik:MaterialResource ResourceKey=ComplementaryBrush}"/>
                            <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth3"/>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="NormalGlyph" Property="Opacity" Value="{telerik1:MaterialResource ResourceKey=DisabledOpacity}"/>
                            <Setter TargetName="BackGlyph" Property="Opacity" Value="{telerik1:MaterialResource ResourceKey=DisabledOpacity}"/>
                        </Trigger>
                        <Trigger Property="FlowDirection" Value="RightToLeft">
                            <Setter Property="BackContentTemplate">
                                <Setter.Value>
                                    <DataTemplate>
                                        <TextBlock
                                                Text="{StaticResource GlyphArrowRight}"
                                                FontSize="32"
                                                FontFamily="{StaticResource TelerikWebUI}"
                                                FontWeight="Normal"
                                                FontStyle="Normal"
                                                Foreground="{Binding Foreground, RelativeSource={RelativeSource AncestorType={x:Type telerikRadialMenu:RadialMenuButton}}}"
                                                Margin="0 4 0 0"/>
                                    </DataTemplate>
                                </Setter.Value>
                            </Setter>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
