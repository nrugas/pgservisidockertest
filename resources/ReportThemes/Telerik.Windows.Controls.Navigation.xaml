        <Setter Property="mat:MaterialAssist.FocusBrush" Value="{telerik1:MaterialResource ResourceKey=PrimaryFocusBrush}"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="Padding" Value="8 0"/>
        <Setter Property="MinHeight" Value="36"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
    </Style>
    <Style TargetType="telerikNavigation:RadBreadcrumb" BasedOn="{StaticResource RadBreadcrumbStyle}"/>
    <Style TargetType="telerikBreadcrumb:RadBreadcrumbBar" BasedOn="{StaticResource RadBreadcrumbBarStyle}"/>
    <Style TargetType="telerikBreadcrumb:RadBreadcrumbBarItem" BasedOn="{StaticResource RadBreadcrumbBarItemStyle}"/>
    <Style x:Key="DesktopAlertCloseButtonStyle" TargetType="telerik:RadButton" BasedOn="{StaticResource RadButtonStyle}">
        <Setter Property="mat:MaterialAssist.MouseOverBrush" Value="{telerik1:MaterialResource ResourceKey=PrimaryHoverBrush}"/>
        <Setter Property="mat:MaterialAssist.PressedBrush" Value="{telerik1:MaterialResource ResourceKey=PrimaryPressedBrush}"/>
        <Setter Property="Padding" Value="10 10 9 10"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="MinHeight" Value="36"/>
        <Setter Property="MinWidth" Value="36"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Bottom"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="CornerRadius" Value="0"/>
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth1"/>
                <Setter Property="Background" Value="{telerik1:MaterialResource ResourceKey=AlternativeBrush}"/>
                <Setter Property="Foreground" Value="{telerik1:MaterialResource ResourceKey=MarkerInvertedBrush}"/>
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth2"/>
                <Setter Property="Background" Value="{telerik1:MaterialResource ResourceKey=AlternativeBrush}"/>
            </Trigger>
        </Style.Triggers>
    </Style>
    <DataTemplate x:Key="DesktopAlertDropDownContent">
        <TextBlock
                FontSize="16"
                FontStyle="Normal"
                FontWeight="Normal"
                telerik:GlyphAdorner.ShowGlyphInDesignTime="True"
                FontFamily="{StaticResource TelerikWebUI}"
                VerticalAlignment="Center"
                HorizontalAlignment="Center"
                Foreground="{TemplateBinding TextElement.Foreground}">
            <Run Text="{StaticResource GlyphArrow60Down}"/>
        </TextBlock>
    </DataTemplate>
    <Style x:Key="DesktopAlertDropDownButtonStyle" TargetType="telerik:RadDropDownButton" BasedOn="{StaticResource RadDropDownButtonStyle}">
        <Setter Property="mat:MaterialAssist.MouseOverBrush" Value="{telerik1:MaterialResource ResourceKey=PrimaryHoverBrush}"/>
        <Setter Property="mat:MaterialAssist.PressedBrush" Value="{telerik1:MaterialResource ResourceKey=PrimaryPressedBrush}"/>
        <Setter Property="mat:MaterialAssist.CheckedBrush" Value="{x:Null}"/>
        <Setter Property="CornerRadius" Value="0"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="MinHeight" Value="36"/>
        <Setter Property="MinWidth" Value="36"/>
        <Setter Property="Padding" Value="10 10 10 9"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="BorderBrush" Value="{x:Null}"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="DropDownIndicatorVisibility" Value="Collapsed"/>
        <Setter Property="ContentTemplate" Value="{StaticResource DesktopAlertDropDownContent}"/>
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Foreground" Value="{telerik1:MaterialResource ResourceKey=MarkerInvertedBrush}"/>
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="Foreground" Value="{telerik1:MaterialResource ResourceKey=MarkerInvertedBrush}"/>
            </Trigger>
            <Trigger Property="IsOpen" Value="True">
                <Setter Property="Foreground" Value="{telerik1:MaterialResource ResourceKey=IconBrush}"/>
            </Trigger>
        </Style.Triggers>
