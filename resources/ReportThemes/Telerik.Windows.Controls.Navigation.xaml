    <ControlTemplate TargetType="telerikNavigation:RadPanelBarItem" x:Key="PanelBarItemTopLevelTemplate">
        <Grid x:Name="RootElement">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <mat:Shadow Grid.RowSpan="2" ShadowDepth="{TemplateBinding mat:MaterialAssist.ShadowDepth}" Background="{TemplateBinding Background}"/>
