    <Style x:Key="BreadcrumbSplitButtonStyle" TargetType="telerik:RadSplitButton" BasedOn="{StaticResource RadSplitButtonStyle}">
        <Setter Property="CornerRadius" Value="0"/>
        <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth0"/>
        <Setter Property="Background" Value="{telerik1:MaterialResource ResourceKey=MainBrush}"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="IsBackgroundVisible" Value="False"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="VerticalAlignment" Value="Stretch"/>
        <Setter Property="Padding" Value="4 0"/>
        <Setter Property="ButtonPartStyle" Value="{StaticResource BreadCrumbButtonStyle}"/>
        <Setter Property="TogglePartStyle" Value="{StaticResource BreadCrumbToggleButtonStyle}"/>
        <Setter Property="DropDownIndicatorVisibility" Value="Collapsed"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:RadSplitButton">
                    <Grid SnapsToDevicePixels="True">
                        <mat:Shadow x:Name="Shadow" Background="{TemplateBinding Background}" CornerRadius="{TemplateBinding CornerRadius}"/>
                        <Border x:Name="BorderVisual"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                CornerRadius="{TemplateBinding CornerRadius}"
                                Background="{x:Null}"
                                SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
                                IsHitTestVisible="False"/>
                        <mat:MaterialControl IsSmartClipped="True" IsRippleEnabled="False" CornerRadius="{TemplateBinding CornerRadius}">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="Auto"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>
                                <telerik:RadButton x:Name="ButtonPart"
                                        Grid.Row="1"
                                        Grid.Column="1"
                                        Style="{TemplateBinding ButtonPartStyle}"
                                        ContentStringFormat="{TemplateBinding ContentStringFormat}"
                                        ContentTemplateSelector="{TemplateBinding ContentTemplateSelector}"
                                        Content="{TemplateBinding Content}"
                                        IsBackgroundVisible="{TemplateBinding IsBackgroundVisible}"
                                        ContentTemplate="{TemplateBinding ContentTemplate}"/>
                                <telerik:RadToggleButton x:Name="DropDownPart"
                                        Grid.Row="1"
                                        Grid.Column="2"
                                        IsChecked="{Binding Path=IsOpen, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}"
                                        Style="{TemplateBinding TogglePartStyle}"
                                        IsBackgroundVisible="{TemplateBinding IsBackgroundVisible}"
                                        Visibility="{TemplateBinding DropDownIndicatorVisibility}">
                                    <telerik:RadToggleButton.Content>
                                        <TextBlock x:Name="arrow"
                                                IsHitTestVisible="False"
                                                Opacity="{telerik1:MaterialResource ResourceKey=SecondaryOpacity}"
                                                telerik:GlyphAdorner.ShowGlyphInDesignTime="True"
                                                FontWeight="Normal"
                                                FontStyle="Normal"
                                                FontSize="16"
                                                Foreground="{Binding Foreground, RelativeSource={RelativeSource AncestorType={x:Type telerik:RadToggleButton}}}"
                                                FontFamily="{StaticResource TelerikWebUI}">
                                            <Run x:Name="arrowRun" Text="{StaticResource GlyphArrowChevronRight}"/>
                                        </TextBlock>
                                    </telerik:RadToggleButton.Content>
                                </telerik:RadToggleButton>
                            </Grid>
                        </mat:MaterialControl>
                        <Popup x:Name="DropDownPopup"
                                AllowsTransparency="True"
                                StaysOpen="False"
                                Focusable="False"
                                Placement="Bottom"
                                PopupAnimation="{TemplateBinding PopupAnimation}"
                                HorizontalOffset="0"
                                VerticalOffset="0">
                            <Grid Margin="8 0 8 8" x:Name="PopupGrid">
                                <mat:Shadow x:Name="PopupShadow"
                                        Height="{TemplateBinding DropDownHeight}"
                                        Width="{TemplateBinding DropDownWidth}"
                                        CornerRadius="{telerik1:MaterialResource ResourceKey=CornerRadiusBottom}"
                                        SnapsToDevicePixels="True"
                                        BorderThickness="{TemplateBinding BorderThickness}"
                                        BorderBrush="{TemplateBinding BorderBrush}"
                                        Background="{telerik1:MaterialResource ResourceKey=MainBrush}"
                                        ShadowDepth="Depth2"/>
                                <Border x:Name="DropDownPopupBorder"
                                        BorderThickness="0 1 0 0"
                                        MinWidth="3"
                                        MinHeight="3"
                                        Width="{TemplateBinding DropDownWidth}"
                                        Height="{TemplateBinding DropDownHeight}"
                                        MaxWidth="{TemplateBinding DropDownMaxWidth}"
                                        MaxHeight="{TemplateBinding DropDownMaxHeight}"
                                        Background="{telerik1:MaterialResource ResourceKey=MainBrush}"
                                        BorderBrush="{telerik1:MaterialResource ResourceKey=DividerBrush}"
                                        CornerRadius="{telerik1:MaterialResource ResourceKey=CornerRadiusBottom}">
                                    <ContentControl x:Name="DropDownPopupContent"
                                            Content="{TemplateBinding DropDownContent}"
                                            ContentTemplate="{TemplateBinding DropDownContentTemplate}"
                                            HorizontalContentAlignment="Stretch"
                                            VerticalContentAlignment="Stretch"
                                            HorizontalAlignment="Stretch"
                                            VerticalAlignment="Stretch"
                                            DataContext="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=DataContext}"/>
                                </Border>
                            </Grid>
                        </Popup>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsOpen" Value="False"/>
                                <Condition Property="IsPressed" Value="False" SourceName="DropDownPart"/>
                                <Condition Property="IsPressed" Value="False" SourceName="ButtonPart"/>
                            </MultiTrigger.Conditions>
                            <Setter Property="Background" Value="Transparent"/>
                        </MultiTrigger>

                        <!-- Is any of the parts pressed -->
                        <Trigger Property="IsPressed" Value="True" SourceName="DropDownPart">
                            <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth1"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True" SourceName="ButtonPart">
                            <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth1"/>
                        </Trigger>

                        <!-- disabled -->
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="DropDownPart" Property="Foreground" Value="{telerik1:MaterialResource ResourceKey=MarkerBrush}"/>
                        </Trigger>

                        <!-- enabled & opened -->
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsEnabled" Value="True"/>
                                <Condition Property="IsOpen" Value="True"/>
                            </MultiTrigger.Conditions>
                            <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth1"/>
                            <Setter Property="Foreground" TargetName="DropDownPart" Value="{telerik1:MaterialResource ResourceKey=IconBrush}"/>
                            <Setter TargetName="arrowRun" Property="Text" Value="{StaticResource GlyphArrowChevronDown}"/>
                        </MultiTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
