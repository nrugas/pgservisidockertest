    <Style x:Key="DecreaseHandleStyle" TargetType="RepeatButton" BasedOn="{StaticResource RepeatButtonStyle}">
        <Setter Property="TextOptions.TextRenderingMode" Value="ClearType"/>
        <Setter Property="Margin" Value="0 0 10 0"/>
        <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth0"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="Padding" Value="10"/>
        <Setter Property="Cursor" Value="Hand"/>
        <Setter Property="IsTabStop" Value="True"/>
        <Setter Property="RenderTransformOrigin" Value=".5 .5"/>
        <Setter Property="ContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <TextBlock x:Name="glyph"
                            telerik1:GlyphAdorner.ShowGlyphInDesignTime="True"
                            FontFamily="{StaticResource TelerikWebUI}"
                            FontWeight="Normal"
                            FontStyle="Normal"
                            Foreground="{telerik1:MaterialResource ResourceKey=IconBrush}"
                            VerticalAlignment="Center"
