    <Style x:Key="BusyIndicatorProgressBarStyle" TargetType="telerik1:RadProgressBar">
        <Setter Property="Margin" Value="0 18"/>
        <Setter Property="Maximum" Value="100"/>
        <Setter Property="Orientation" Value="Horizontal"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik1:RadProgressBar">
                    <Grid HorizontalAlignment="Center" Width="180">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Determinate">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="ProgressBarSpinnerEllipses" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Collapsed</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Indeterminate">
                                    <Storyboard RepeatBehavior="Forever">
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="FullyIndeterminateGridScaleTransform" Storyboard.TargetProperty="ScaleX" RepeatBehavior="Forever">
                                            <SplineDoubleKeyFrame KeyTime="0" Value="0.0"/>
                                            <SplineDoubleKeyFrame KeyTime="0:0:0.4" Value="0.95"/>
                                            <SplineDoubleKeyFrame KeyTime="0:0:2" Value="0.05"/>
                                            <SplineDoubleKeyFrame KeyTime="0:0:2.4" Value="0.95"/>
                                            <SplineDoubleKeyFrame KeyTime="0:0:4" Value="0.05"/>
                                            <SplineDoubleKeyFrame KeyTime="0:0:4.4" Value="0.95"/>
                                            <SplineDoubleKeyFrame KeyTime="0:0:6" Value="0.0"/>
                                            <SplineDoubleKeyFrame KeyTime="0:0:6.3" Value="0.0"/>
                                        </DoubleAnimationUsingKeyFrames>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="ellipseRotate" Storyboard.TargetProperty="Angle" RepeatBehavior="Forever">
                                            <SplineDoubleKeyFrame KeyTime="0:0:0.4" Value="120"/>
                                            <SplineDoubleKeyFrame KeyTime="0:0:2" Value="840"/>
                                            <SplineDoubleKeyFrame KeyTime="0:0:2.4" Value="960"/>
                                            <SplineDoubleKeyFrame KeyTime="0:0:4" Value="1680"/>
                                            <SplineDoubleKeyFrame KeyTime="0:0:4.4" Value="1800"/>
                                            <SplineDoubleKeyFrame KeyTime="0:0:6" Value="2520"/>
                                            <SplineDoubleKeyFrame KeyTime="0:0:6.3" Value="0"/>
                                        </DoubleAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="ProgressBarTrack" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Collapsed</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>

                        <!-- In order for the converter to work properly, whis should be square -> Width = Height -->
                        <Grid x:Name="ProgressBarSpinnerEllipses" Margin="0 0 0 18" Width="64" Height="64">
                            <Grid x:Name="FullyIndeterminateGrid">
                                <Grid.RenderTransform>
                                    <ScaleTransform x:Name="FullyIndeterminateGridScaleTransform" ScaleX="0"/>
                                </Grid.RenderTransform>
                            </Grid>
                            <Canvas RenderTransformOrigin=".5 .5">
                                <Canvas.RenderTransform>
                                    <RotateTransform x:Name="ellipseRotate"/>
                                </Canvas.RenderTransform>
                                <Path Stroke="{telerik1:MaterialResource ResourceKey=PrimaryNormalBrush}" StrokeThickness="4">
                                    <Path.Data>
                                        <MultiBinding Converter="{StaticResource DoubleToArcConverter}" Mode="OneWay">
                                            <Binding ElementName="FullyIndeterminateGridScaleTransform" Path="ScaleX" Mode="OneWay"/>
                                            <Binding ElementName="ProgressBarSpinnerEllipses" Path="ActualWidth" Mode="OneWay"/>
                                        </MultiBinding>
                                    </Path.Data>
                                </Path>
                            </Canvas>
                        </Grid>
                        <Grid x:Name="ProgressBarTrack" Height="6" Background="{telerik1:MaterialResource ResourceKey=AlternativeBrush}" RenderTransformOrigin="0.5 0.5" Grid.Row="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <Rectangle x:Name="SkipValueSpacer"/>
                            <Rectangle x:Name="ProgressBarIndicator" Grid.Column="1" Fill="{telerik1:MaterialResource ResourceKey=PrimaryNormalBrush}"/>
                        </Grid>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadBusyIndicatorStyle" TargetType="telerik1:RadBusyIndicator">
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="DisplayAfter" Value="00:00:00.1"/>
        <Setter Property="Background" Value="{telerik1:MaterialResource ResourceKey=MainBrush}"/>
