                                Visibility="Collapsed"
                                BorderThickness="0 0 0 3"/>
                        <Border x:Name="FocusVisual" Background="{x:Null}" BorderBrush="{TemplateBinding mat:MaterialAssist.FocusBrush}" BorderThickness="0 0 0 3" RenderTransformOrigin="0.5 1">
                            <Border.RenderTransform>
                                <ScaleTransform ScaleX="0" ScaleY="1"/>
